{
  "id": null,
  "uid": "spring-metrics",
  "title": "Spring Boot metrics",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "JVM Memory Used",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "{{area}}"
        }
      ],
      "lines": true
    },
    {
      "type": "graph",
      "title": "HTTP Requests",
      "targets": [
        {
          "expr": "http_server_requests_seconds_count",
          "legendFormat": "{{uri}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "System CPU Usage",
      "targets": [
        {
          "expr": "system_cpu_usage",
          "legendFormat": "CPU"
        }
      ]
    }
  ]
}